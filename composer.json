{
    "name": "wijzijnweb/laravel-inertia-permissions",
    "description": "Easy to use package to implement Spatie's Permissions package into Inertia Laravel projects.",
    "keywords": [
        "permissions",
        "roles",
        "inertiajs",
        "laravel",
        "Spatie"
    ],
    "homepage": "https://github.com/wijzijnweb/laravel-inertia-permissions",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "spatie/laravel-permission": "^6.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Wijzijnweb\\LaravelInertiaPermissions\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wijzijnweb\\LaravelInertiaPermissions\\PermissionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
